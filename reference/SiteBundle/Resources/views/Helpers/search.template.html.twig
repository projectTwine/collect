    <a class="type-{{payload.type}} suggestion-template template-uni" href="{{payload.url}}" data-type="{{payload.type}}" data-name="{{payload.name}}" onmousedown="ga('send','event', 'Search Autocomplete' , '{{payload.type}}', '{{payload.name}}' )">
        <span class="name">{{payload.name}}</span>
        {{#if payload.count}}
        <span class="course-count">{{payload.count}} courses</span>
        {{/if}}
    </a>
