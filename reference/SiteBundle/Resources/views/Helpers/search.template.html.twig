        {{/if}}
        <div class="session-wrap">
            {{#if payload.nextSession}}
            <span class="next-session">{{payload.nextSession}}</span>
            {{/if}}
            {{#if payload.provider}}
            <span class="course-provider">&nbsp;via {{payload.provider}}</span>
            {{/if}}
        </div>
    </a>
</script>
{% endraw %}
<!-- End Handlebars template scripts for Typeahead-->
